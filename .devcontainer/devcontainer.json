{
    "name": "React Template with Vite and Deno",
    "image": "ghcr.io/devcontainers/javascript-node:latest",
    "forwardPorts": [
      3000,
      8000
    ],
    "customizations": {
      "vscode": {
        "settings": {
          "deno.enable": true,
          "deno.lint": true
        },
        "extensions": [
          "denoland.vscode-deno",
          "esbenp.prettier-vscode",
          "ms-vscode.vscode-typescript-tslint-plugin"
        ]
      }
    },
    "postCreateCommand": "deno install"
  }